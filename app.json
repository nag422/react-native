{
  "expo": {
    "name": "DigitalUpgrade",
    "slug": "digital-upgrade",
    "sdkVersion": "41.0.0",
    "version": "4.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kiranvoleti.digitalupgrade"
    },
    "android": {
      "package": "com.kiranvoleti.digitalupgrade",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
    
  }
}
